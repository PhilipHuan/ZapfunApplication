<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/loginFragment">
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.zapfunfoodorderingapplication.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_myMenuFragment"
            app:destination="@id/myMenuFragment" />
    </fragment>
    <fragment
        android:id="@+id/myMenuFragment"
        android:name="com.example.zapfunfoodorderingapplication.MyMenuFragment"
        android:label="fragment_my_menu"
        tools:layout="@layout/fragment_my_menu" />
    <fragment
        android:id="@+id/myHistoryFragment"
        android:name="com.example.zapfunfoodorderingapplication.MyHistoryFragment"
        android:label="fragment_my_history"
        tools:layout="@layout/fragment_my_history" />
    <fragment
        android:id="@+id/myOrderFragment"
        android:name="com.example.zapfunfoodorderingapplication.MyOrderFragment"
        android:label="MyOrderFragment"
        tools:layout="@layout/fragment_my_order"/>
</navigation>